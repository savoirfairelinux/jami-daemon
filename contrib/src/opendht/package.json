{
    "name": "opendht",
    "version": "v3.7.2",
    "url": "https://github.com/savoirfairelinux/opendht/archive/__VERSION__.tar.gz",
    "deps": [
        "argon2",
        "asio",
        "gnutls",
        "jsoncpp",
        "msgpack",
        "restinio",
        "simdutf"
    ],
    "use_cmake": true,
    "defines": [
        "CMAKE_CXX_STANDARD=20",
        "BUILD_SHARED_LIBS=0",
        "BUILD_TESTING=0",
        "OPENDHT_DOCUMENTATION=0",
        "OPENDHT_SHARED=0",
        "OPENDHT_PROXY_CLIENT=1",
        "OPENDHT_PROXY_SERVER=1",
        "OPENDHT_PUSH_NOTIFICATIONS=1",
        "OPENDHT_TOOLS=0",
        "CMAKE_MSVC_RUNTIME_LIBRARY=MultiThreadedDLL"
    ],
    "patches": [
        "0001-cmake-fix-jami-contrib-linking.patch"
    ],
    "win_patches": [],
    "project_paths": [],
    "with_env": "",
    "custom_scripts": {
        "pre_build": [],
        "build": [],
        "post_build": []
    }
