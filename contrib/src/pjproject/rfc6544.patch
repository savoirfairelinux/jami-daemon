+    pjnath_perror(ice->obj_name, "Error sending STUN request", status_send_msg);
+    pj_log_pop_indent();
+  }
+  check_set_state(ice, check, PJ_ICE_SESS_CHECK_STATE_IN_PROGRESS, status);
